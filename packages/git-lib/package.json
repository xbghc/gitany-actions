{
  "name": "@gitany/git-lib",
  "version": "0.1.0",
  "description": "Minimal git command wrappers for gitany.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "node ./scripts/build.mjs",
    "clean": "rm -rf dist",
    "lint": "eslint ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "dependencies": {
    "cross-spawn": "^7.0.3"
  },
  "devDependencies": {
    "esbuild": "^0.21.5",
    "@types/cross-spawn": "^6.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
