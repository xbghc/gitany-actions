{
  "name": "@gitany/cli",
  "version": "0.1.0",
  "description": "CLI for gitany projects.",
  "type": "module",
  "bin": {
    "gitcode": "dist/index.js"
  },
  "files": ["dist"],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@gitany/gitcode": "workspace:*"
  },
  "engines": { "node": ">=18.17" }
}

