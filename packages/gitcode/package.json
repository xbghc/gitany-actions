{
  "name": "@gitany/gitcode",
  "version": "0.1.0",
  "description": "Git utilities for the gitany monorepo.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "lint": "eslint ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "engines": { "node": ">=18.17" }
}

