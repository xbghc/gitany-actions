{
  "name": "@gitany/gitcode",
  "version": "0.1.0",
  "description": "Git utilities for the gitany monorepo.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "node ./scripts/build.mjs",
    "clean": "rm -rf dist",
    "lint": "eslint ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "devDependencies": {
    "esbuild": "^0.21.5"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@gitany/shared": "workspace:*",
    "got": "^14.4.9",
    "zod": "^4.1.7"
  }
}
