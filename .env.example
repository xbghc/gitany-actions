# GitAny / GitCode 访问令牌
GITANY_TOKEN=your-token
GITCODE_TOKEN=your-token

# GitCode API 配置
GITCODE_API_BASE=https://gitcode.com/api/v5
GITCODE_AUTH_STYLE=bearer
GITCODE_AUTH_HEADER=

# HTTP 调试
GITCODE_HTTP_DEBUG=
GITCODE_HTTP_DEBUG_SHOW_SECRETS=
GITANY_HTTP_DEBUG=

# 日志级别（使用 GITANY_LOG_LEVEL）
GITANY_LOG_LEVEL=

# 包管理器镜像 (可选)
NPM_CONFIG_REGISTRY=
PNPM_CONFIG_REGISTRY=
